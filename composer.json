{
    "name": "sikofitt/generate-mac",
    "description": "Generates dummy mac addresses",
    "type": "library",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ext-sodium": "*"
    },
    "autoload": {
      "psr-4": {
        "Sikofitt\\GenerateMac\\": "src/GenerateMac"
      }
    },
    "require-dev": {
        "composer/xdebug-handler": "^3.0",
        "friendsofphp/php-cs-fixer": "^3.11",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/console": "^5.2"
    },
    "autoload-dev": {
      "psr-4": {
        "Sikofitt\\GenerateMac\\Tests\\": "tests/"
      }
    },

    "config": {
      "sort-packages": true
    },
    "suggest": {
      "symfony/console": "To use the generate-mac command."
    },
    "bin": ["bin/generate-mac"],
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "sikofitt",
            "email": "sikofitt@gmail.com"
        }
    ]
}
