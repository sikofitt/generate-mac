{
    "name": "sikofitt/generate-mac",
    "description": "Generates dummy mac addresses",
    "type": "library",
    "require": {
      "php": "^7.2",
      "ext-sodium": "*",
      "ext-json": "*"
    },
    "autoload": {
      "psr-4": {
        "Sikofitt\\GenerateMac\\": "src/GenerateMac"
      }
    },
    "require-dev": {
        "symfony/console": "^4.1",
        "phpunit/phpunit": "^7.4",
        "friendsofphp/php-cs-fixer": "^2.13",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload-dev": {
      "psr-4": {
        "Sikofitt\\GenerateMac\\Tests\\": "tests/"
      }
    },
    "config": {
      "sort-packages": true
    },
    "suggest": {
      "symfony/console": "To use the generate-mac command."
    },
    "bin": ["bin/generate-mac"],
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "sikofitt",
            "email": "sikofitt@gmail.com"
        }
    ]
}
